{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mld",
      "camelcase": "Mld",
      "title": "Mld",
      "scope": "source.mld",
      "file-types": [
        "mld"
      ],
      "injection-regex": "^mld$",
      "class-name": "TreeSitterMld"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for ocaml-mld files",
    "authors": [
      {
        "name": "Etienne Millon"
      }
    ],
    "links": {
      "repository": "https://github.com/emillon/tree-sitter-mld"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}